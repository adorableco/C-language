#include <stdio.h>

int main()
{
    int num,sum = 0;
    scanf("%d",&num);
    int arr[num];
    
    for (int i = 0; i < num; i++)
    {
        scanf("%d",&arr[i]);
    }
    
    for (int i = 0; i < num; i++)
    {
        if(arr[i]!=1)
            sum += (arr[i]-1);
    }

    printf("%d\n",sum+1);
    return 0;
}
